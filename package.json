{
  "name": "lazy-i18n",
  "version": "1.0.0",
  "author": "Charles Stover <lazy-i18n@charlesstover.com>",
  "description": "Lazy loads translations for React apps",
  "homepage": "https://www.npmjs.com/package/lazy-i18n",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "repository": "github:CharlesStover/lazy-i18n",
  "type": "module",
  "types": "./types/index.d.ts",
  "bugs": {
    "email": "lazy-i18n@charlesstover.com",
    "url": "https://github.com/CharlesStover/lazy-i18n/issues"
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.mjs"
  },
  "scripts": {
    "build": "rollup --config",
    "eslint": "eslint",
    "jest": "jest",
    "start": "rollup --config --watch",
    "test": "eslint && jest",
    "test-watch": "jest --coverage=false --watch",
    "yarn-upgrade": "yarn up * && yarn up @*/*",
    "yarn-vscode": "yarn dlx @yarnpkg/pnpify --sdk vscode"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/testing-library__react-hooks": "^3.4.1",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.35.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  }
